{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue","src/utils/*.ts"],
  "exclude": ["src/**/__tests__/*"],
  "types": [
    "leaflet",
    "geojson"
  ],
  "compilerOptions": {
